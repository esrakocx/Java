
public class Main {
	public static void main(String[] args) {
		
		String a = "Hello";
		System.out.println(a + " Welcome!");
		
		String b = "Java ";
		String c = "Programming ";
		String d = "Language";
		
		String sum = b + c + d;
		System.out.println("Sum: " + sum);
		
		String e = "Hi";
		int f = 34;
		String g = e + f;
		System.out.println(g);
	}

}
