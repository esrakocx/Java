
public class Main {
	public static void main(String[] args) {
		
		/* 
		   + --> addition
		   - --> subtraction  
		   / --> division
		   * --> multiplication
		   % --> remainder	
		 
		 */ 
		
		System.out.println(3 + 4.4);
		System.out.println(3 - 5f);
		System.out.println(10 / 4);
		System.out.println(10d / 4);
		System.out.println(10 / 4.0);
		System.out.println(3 * 4.5);
		System.out.println(10 % 4);
	}
}
